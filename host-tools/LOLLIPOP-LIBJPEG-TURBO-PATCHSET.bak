#!/bin/sh

. $(dirname $0)/functions

PATCHES=0
if [ -n "$1" ]; then
    AOSP="$1"
else
    AOSP="`pwd`"
fi

if ! [ -d "$AOSP" ] && ! [ -d "$AOSP/bionic" ] && [ -d "$AOSP"/build ]; then
    echo "This script must be run from the AOSP source directory"
    echo "or with the AOSP source directory as its first parameter."
    exit 1
fi

##################################################
################ Non-upstreamable ################
##################################################
apply --linaro external/skia 15592/1
apply --linaro external/opencv 15593/1
apply --linaro frameworks/base 15594/1
apply --linaro frameworks/av 15595/1
apply --linaro packages/apps/Camera2 15596/1
apply --linaro packages/apps/Gallery2 15597/1
apply --linaro external/jpeg 15598/1
